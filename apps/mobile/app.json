{
  "expo": {
    "name": "Leace",
    "slug": "leace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash_rounded.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daaak.leace"
    },
    "web": {
      "favicon": "./assets/logo_1024.png"
    },
    "plugins": [
      ["expo-document-picker"],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "CLERK_PUBLISHABLE_KEY": "pk_test_Y2VydGFpbi1jcmFiLTU0LmNsZXJrLmFjY291bnRzLmRldiQ"
    }
  }
}
