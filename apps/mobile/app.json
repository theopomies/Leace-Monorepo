{
  "expo": {
    "name": "Leace",
    "slug": "leace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash_rounded.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      }
    },
    "web": {
      "favicon": "./assets/logo_1024.png"
    },
    "plugins": [
      ["expo-document-picker"],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "scheme": "com.leaceeip.leace",
    "extra": {
      "eas": {
        "projectId": "2eb16b83-91bd-4533-856e-cef45685028c"
      },
      "CLERK_PUBLISHABLE_KEY": "pk_test_Y2VydGFpbi1jcmFiLTU0LmNsZXJrLmFjY291bnRzLmRldiQ"
    },
    "android": {
      "package": "com.leaceeip.leace",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    }
  }
}
